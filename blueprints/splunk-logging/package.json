{
  "name": "splunk-logging",
  "version": "0.8.1",
  "description": "AWS Lambda function blueprint to demonstrate logging from AWS Lambda code to Splunk's HTTP event collector",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c ../.eslintrc --ignore-path ../.eslintignore .",
    "test": "node integration-test.js",
    "build": "zip -r splunk-logging.zip index.js lambda.json lib/",
    "clean": "rm -f splunk-logging.zip",

    "postinstall": "node -e \"var s='../lib',d='lib',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'dir')});\"",
    "pretest": "npm run lint"
  },
  "keywords": [
    "splunk",
    "lambda",
    "logging",
    "hec"
  ],
  "authors": [
    "Roy Arsan <rarsan@splunk.com> (https://www.splunk.com)",
    "Glenn Block <gblock@splunk.com> (https://www.splunk.com)"
  ],
  "license": "CC0-1.0",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1"
  }
}
